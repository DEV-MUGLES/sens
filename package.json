{
  "name": "@pickk/sens",
  "version": "1.1.0",
  "description": "NCP SENS client for nodejs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DEV-MUGLES/sens.git"
  },
  "keywords": [
    "sens",
    "naver",
    "nodejs",
    "kakaotalk"
  ],
  "author": "DEV-MUGLES",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DEV-MUGLES/sens/issues"
  },
  "homepage": "https://github.com/DEV-MUGLES/sens#readme",
  "scripts": {
    "build": "rimraf -rf dist && tsc -p tsconfig.json",
    "format": "prettier --write **/*.{ts,json} --ignore-path ./.prettierignore",
    "lint": "eslint \"lib/**/*.ts\" --fix",
    "prepublish:next": "npm run build",
    "publish:next": "npm publish --access public --tag next",
    "prepublish:npm": "npm run format && npm run lint && npm run build",
    "publish:npm": "npm publish --access public",
    "prerelease": "npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "coverage": "jest --coverage"
  },
  "dependencies": {
    "axios": "^0.21.1"
  },
  "devDependencies": {
    "@types/axios": "0.14.0",
    "@types/faker": "5.5.8",
    "@types/jest": "26.0.24",
    "@types/node": "14.17.19",
    "@typescript-eslint/eslint-plugin": "4.26.0",
    "@typescript-eslint/parser": "4.26.0",
    "eslint": "7.27.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.23.4",
    "faker": "5.5.3",
    "jest": "26.6.3",
    "prettier": "2.3.0",
    "rimraf": "3.0.2",
    "ts-jest": "26.5.6",
    "typescript": "4.3.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage"
  },
  "directories": {
    "lib": "lib",
    "test": "tests"
  }
}
